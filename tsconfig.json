        "./src/typescript/audiobus/instruments/basics/SineWave.ts",
        "./src/typescript/audiobus/instruments/basics/SquareWave.ts",
        "./src/typescript/audiobus/instruments/basics/TriangleWave.ts",
        "./src/typescript/audiobus/instruments/beats/BassDrum.ts",
