        "./src/typescript/audiobus/instruments/beats/HiHat.ts",
        "./src/typescript/audiobus/instruments/beats/Snare.ts",
        "./src/typescript/audiobus/instruments/beats/Tom.ts",
        "./src/typescript/audiobus/instruments/DrumMachine.ts",
        "./src/typescript/audiobus/instruments/DrumSet.ts",
        "./src/typescript/audiobus/instruments/Instrument.ts",
        "./src/typescript/audiobus/instruments/OscillatorTypes.ts",
        "./src/typescript/audiobus/instruments/Synthesizer.ts",
        "./src/typescript/audiobus/instruments/Theremin.ts",
        "./src/typescript/audiobus/instruments/Violin.ts",
