{
    "version": "1.8.0",
    "compilerOptions": {
        "allowUnusedLabels": false,
        "declaration": false,
        "module": "commonjs",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noLib": false,
        "preserveConstEnums": true,
        "removeComments": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5"
    },
    "compileOnSave": false,
    "filesGlob": [
        "./src/typescript/audiobus/**/*.ts",
        "./typings/*.ts",
        "./src/typescript/examples/**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./src/typescript/audiobus/Conductor.ts",
        "./src/typescript/audiobus/Dependencies.ts",
        "./src/typescript/audiobus/Engine.ts",
        "./src/typescript/audiobus/envelopes/Envelope.ts",
        "./src/typescript/audiobus/IMixerItem.ts",
        "./src/typescript/audiobus/instruments/basics/CustomWave.ts",
        "./src/typescript/audiobus/instruments/basics/DualOscillator.ts",
        "./src/typescript/audiobus/instruments/basics/Oscillator.ts",
