        "./src/typescript/audiobus/io/MidiFile.ts",
        "./src/typescript/audiobus/io/MidiHardware.ts",
        "./src/typescript/audiobus/io/MidiStream.ts",
        "./src/typescript/audiobus/io/MidiTrack.ts",
        "./src/typescript/audiobus/IRackItem.ts",
        "./src/typescript/audiobus/ISoundControl.ts",
        "./src/typescript/audiobus/Mixer.ts",
        "./src/typescript/audiobus/Rack.ts",
        "./src/typescript/audiobus/Scales.ts",
        "./src/typescript/audiobus/timing/Metronome.ts",
        "./src/typescript/audiobus/timing/Netronome.ts",
